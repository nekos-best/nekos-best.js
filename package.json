{
    "name": "nekos-best.js",
    "version": "4.0.5",
    "description": "The official JavaScript wrapper for the https://nekos.best API with TypeScript typings",
    "typings": "index.d.ts",
    "files": [
        "index.*"
    ],
    "exports": {
        "import": "./index.mjs",
        "require": "./index.js"
    },
    "license": "MIT",
    "bugs": "https://github.com/nekos-best/nekos-best.js/issues",
    "author": "thunder04 (https://github.com/thunder04)",
    "repository": "github:nekos-best/nekos-best.js",
    "contributors": [
        "oriel-beck (https://github.com/oriel-beck)",
        "Thusuzzee (https://github.com/Thusuzzee)"
    ],
    "keywords": [
        "wrapper",
        "nekos",
        "neko",
        "api"
    ],
    "scripts": {
        "prepublish": "npx esbuild index.ts --outfile=index.js --platform=node --bundle --minify --keep-names --external:petitio --format=cjs",
        "prepack": "npm run prepublish && npm run types",
        "types": "npx tsc --emitDeclarationOnly"
    },
    "dependencies": {
        "petitio": "^1.2.1"
    },
    "devDependencies": {
        "@types/node": "^15.0.3",
        "esbuild": "^0.12.16",
        "typescript": "^4.3.5"
    },
    "engines": {
        "node": ">=12.8.1"
    }
}